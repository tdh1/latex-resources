isbn = {2212-8271},
journal = {Procedia CIRP},
keywords = {Aerospace Engine Components,Design For Manufacturing,Fabrication,Platforms,Producibility,Set-based Engineering,Systems Engineering,Vallhagen:2013},
mendeley-tags = {Vallhagen:2013},
number = {0},
pages = {145--150},
title = {{A Framework for Producibility and Design for Manufacturing Requirements in a System Engineering Context}},
