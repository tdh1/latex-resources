keywords = {Quality Information Framework (QIF),XML schema,Zhao:2012,data model,interoperability,manufacturing quality,measurement features,quality characteristics},
mendeley-tags = {Zhao:2012},
pages = {8 pp.},
title = {{Quality Information Framework ‹ Integrating Metrology Processes}},
url = {http://www.nist.gov/manuscript-publication-search.cfm?pub{\_}id=910396},
year = {2012}
}
