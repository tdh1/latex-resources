@article{Kim1999,
abstract = {Companies of sustained high growth and profits all pursue a strategy, value innovation, that renders the competition irrelevant by offering new and superior buyer value in existing markets or by enabling the creation of new markets through quantum leaps in buyer value. Value innovation places equal emphasis on value and innovation, since innovation without value can be too strategic or wild, too technology-driven or futuristic. To value innovate, managers must ask two questions: 1. Is the firm offering customers radically superior value? 2. Is the firm's price level accessible to the mass of buyers in the target market? Five key questions contrasting conventional competition-based logic with that of value innovation are presented and the type of organization that best unlocks employees' ideas and creativity is described.},
address = {Cambridge},
annote = {Copyright - Copyright Sloan Management Review Association, Alfred P. Sloan School of Management Spring 1999
SubjectsTermNotLitGenreText - US
