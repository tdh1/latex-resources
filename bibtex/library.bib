file = {:C$\backslash$:/Users/tomjr/Documents/Mendeley Desktop/Journal of Computing and Information Science in Engineering/Kikuchi et al. - 2010 - PDQ (Product Data Quality) Representation of Data Quality for Product Data and Specifically for Shape Data(2).pdf:pdf;:C$\backslash$:/Users/tomjr/Documents/Mendeley Desktop/Journal of Computing and Information Science in Engineering/Kikuchi et al. - 2010 - PDQ (Product Data Quality) Representation of Data Quality for Product Data and Specifically for Shape Data(3).pdf:pdf;:C$\backslash$:/Users/tomjr/Documents/Mendeley Desktop/Journal of Computing and Information Science in Engineering/Kikuchi et al. - 2010 - PDQ (Product Data Quality) Representation of Data Quality for Product Data and Specifically for Shape Data.pdf:pdf},
isbn = {1530-9827},
