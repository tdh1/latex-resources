@article{Deuter2016,
abstract = {The Internet of Things (IoT) is the main driver for industrial smart products produced by an increasing number of manufacturers. The overall functionality of smart products is a combination of mechanical, electrical/electronic functions (hardware functions) and software functions. For hardware and software there are different lifecycle models: Product lifecycle management (PLM) focuses on hardware, application lifecycle management (ALM) focuses on software. Smart products force manufacturers to converge both lifecycle models step by step. Although seemingly important, the research community leaves this innovative area mostly up to the PLM tool vendors and the ALM tool vendors, resulting in them driving the convergence. This paper points out the mismatch between industry and academia regarding the PLM/ALM convergence. We encourage academia to increase research activities and we propose potential research topics.},
author = {Deuter, Andreas and Rizzo, Stefano},
doi = {10.1016/j.protcy.2016.08.052},
file = {:C$\backslash$:/Users/tomjr/Documents/Mendeley Desktop/Procedia Technology/Deuter, Rizzo - 2016 - A Critical View on PLMALM Convergence in Practice and Research.pdf:pdf},
isbn = {4952617025},
issn = {22120173},
journal = {Procedia Technology},
pages = {405--412},
title = {{A Critical View on PLM/ALM Convergence in Practice and Research}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S2212017316303991},
volume = {26},
year = {2016}
}
@article{Jennings2016,
abstract = {Rapid changes in technology have led to an increasingly fast pace of product introductions. For long-life systems (e.g., planes, ships, and nuclear power plants), rapid changes help sustain useful life, but at the same time, present significant challenges associated with obsolescence management. Over the years, many approaches for forecasting obsolescence risk and product life cycle have been developed. However, gathering inputs required for forecasting is often subjective and laborious, causing inconsistencies in predictions. To address these issues, the objective of this research is to develop a machine learning-based methodology capable of forecasting obsolescence risk and product life cycle accurately while minimizing maintenance and upkeep of the forecasting system. Specifically, this new methodology enables prediction of both the obsolescence risk level and the date when a part becomes obsolete. A case study of the cell phone market is presented to demonstrate the effectiveness and efficiency of the new approach. Results have shown that machine learning algorithms (i.e., random forest, artificial neural networks, and support vector machines) can classify parts as active or obsolete with over 98{\%} accuracy and predict obsolescence dates within a few months.},
author = {Jennings, C and Wu, D and Terpenny, J},
doi = {10.1109/TCPMT.2016.2589206},
file = {:C$\backslash$:/Users/tomjr/Documents/Mendeley Desktop/IEEE Transactions on Components, Packaging and Manufacturing Technology/Jennings, Wu, Terpenny - 2016 - Forecasting Obsolescence Risk and Product Life Cycle With Machine Learning.pdf:pdf},
isbn = {2156-3950},
journal = {IEEE Transactions on Components, Packaging and Manufacturing Technology},
keywords = {Aging,Data models,Diminishing manufacturing sources and material sho,Forecasting,Industries,Jennings:2016,Manufacturing,Market research,Predictive models,cell phone market,cellular radio,electronic parts,learning (artificial intelligence),life cycle stages,machine learning,maintenance engineering,maintenance minimization,obsolescence,obsolescence management,obsolescence risk forecasting,product life cycle forecasting,product life cycle management,risk management,sustainment},
mendeley-tags = {Jennings:2016},
number = {9},
pages = {1428--1439},
title = {{Forecasting Obsolescence Risk and Product Life Cycle With Machine Learning}},
volume = {6},
year = {2016}
}
