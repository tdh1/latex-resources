isbn = {0167-739X},
journal = {Future Generation Computer Systems},
keywords = {Cloud computing,Gubbi:2013,Internet of Things,RFID,Smart environments,Ubiquitous sensing,Wireless sensor networks},
mendeley-tags = {Gubbi:2013},
number = {7},
pages = {1645--1660},
title = {{Internet of Things (IoT): A vision, architectural elements, and future directions}},
url = {http://www.sciencedirect.com/science/article/pii/S0167739X13000241},
volume = {29},
year = {2013}
