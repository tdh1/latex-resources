isbn = {08982112},
journal = {Quality Engineering},
keywords = {Budget control,Commerce,Cost benefit analysis,Costs,Decision making,Design,Design of experiments,Experiments,Lu:2014-2,Mixtures},
mendeley-tags = {Lu:2014-2},
