keywords = {Industry 4.0,Measurement,OWL,Ontologies,Petersen:2016,Reliability,SCORVoc RDFS vocabulary,SPARQL queries,Supply Chain Council SCOR standard,Supply Chain Operation Reference,Supply chains,Vocabulary,cross-industry approach,cyber-physical systems,information systems,ontology editor,semantics-based approaches,smart manufacturing,supply chain,supply chain management,supply chain networks,value-added networks,vocabulary development,vocabulary-based information exchange,vocabulary-based information integration},
mendeley-tags = {Petersen:2016},
pages = {132--139},
