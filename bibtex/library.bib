isbn = {08906955},
journal = {International Journal of Machine Tools and Manufacture},
keywords = {Calibration,Computer simulation,Identification (control systems),Inspection,Lee:1997,Manufacture,Mathematical models,Measurement errors,Precision engineering,Spatial variables measurement},
mendeley-tags = {Lee:1997},
number = {3},
pages = {263--276},
publisher = {Elsevier Science Ltd},
title = {{Analytical assessment of measurement uncertainty in precision inspection and machine calibration}},
url = {http://dx.doi.org/10.1016/S0890-6955(96)00061-2},
